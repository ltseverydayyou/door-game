OrionLib:MakeNotification({
	Name = "Hello",
	Content = "UI still in beta bugs may be expected",
	Image = "rbxassetid://11151804223",
	Time = 5
})

OrionLib:MakeNotification({
	Name = "Notice",
	Content = "If you notice any bugs report it to Cringe#0438",
	Image = "rbxassetid://11151804223",
	Time = 5
})
