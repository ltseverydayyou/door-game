local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Cringe Hub | Doors", HidePremium = false, IntroText = "Cringe Hub", SaveConfig = false, ConfigFolder = "ur mum"})

local Tab = Window:MakeTab({
	Name = "Stuff",
	Icon = "rbxassetid://10838458392",
	PremiumOnly = false
})



local Tab1 = Window:MakeTab({
	Name = "Doors",
	Icon = "rbxassetid://10791126871",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Endless Doors",
	Icon = "rbxassetid://11666287209",
	PremiumOnly = false
})

local Tab3 = Window:MakeTab({
	Name = "Custom Entities",
	Icon = "rbxassetid://11151804223",
	PremiumOnly = false
})

local Tab4 = Window:MakeTab({
	Name = "Rooms",
	Icon = "rbxassetid://11287256498",
	PremiumOnly = false
})

local Tab5 = Window:MakeTab({
	Name = "Troll Entities",
	Icon = "rbxassetid://6862780932",
	PremiumOnly = false
})

local Tab6 = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://8950621656",
	PremiumOnly = false
})

local Tab7 = Window:MakeTab({
	Name = "Destroy UI",
	Icon = "rbxassetid://8463436236",
	PremiumOnly = false
})


Tab:AddButton({
